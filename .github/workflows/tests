name: Test xmod
on:
  push: null
  pull_request: null
  workflow_dispatch: null
jobs:
  build:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v2
      - name: Run a multi-line script
        run: |
          cp tests/script.sh .
          make
          for i in 1 2 3 4 5
          do 
            for j in 1 2 3
            do 
              ./script.sh $j
            done
          done
